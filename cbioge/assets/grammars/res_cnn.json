{
	"name": "Residual CNN 1.0", 

	"rules": {
		"<cnn>": [
			["<layer>"], 
			["<layer>", "<layer>", "<layer>"], 
			["<layer>", "<layer>", "<layer>", "<layer>", "<layer>"]
		], 
		"<layer>": [ 
			["<conv>", "<layer>"], ["<conv>"], 
			["<resblock>", "<layer>"], ["<resblock>"], 
			["<pool>", "<layer>"], ["<pool>"], 
			["<dropout>", "<layer>"], ["<dropout>"], 
			["<dense>", "<layer>"], ["<dense>"], 
			["&"]
		], 
		"<conv>"		: [ ["conv", "<filters>", "<ksize>", "<strides>", "<padding>", "<activation>"] ], 
		"<resblock>"	: [ ["resblock", "<filters>", "<ksize>"] ], 
		"<dense>"		: [ ["dense", "<units>", "<activation>"] ], 
		"<dropout>"		: [ ["dropout", "<rate>"] ], 
		"<pool>"		: [ ["<pool_type>", "<ksize>", "<strides>", "<padding>"] ], 

		"<pool_type>" 	: [ ["maxpool"], ["avgpool"] ], 
		"<activation>"	: [ ["relu"], ["selu"], ["elu"], ["tanh"], ["sigmoid"], ["linear"] ], 
		"<padding>"		: [ ["valid"], ["same"] ], 
		"<filters>"		: [ [16], [32], [64], [128], [256], [512] ], 
		"<strides>"		: [ [1], [2] ], 
		"<ksize>"		: [ [1], [2], [3], [4], [5] ], 
		"<units>"		: [ [32], [64], [128], [256], [512], [1024] ], 
		"<rate>"		: [ ["[0.0,0.5]"] ]
	}, 

	"blocks": {
		"conv": ["Conv2D", "filters", "kernel_size", "strides", "padding", "activation"], 
		"resblock": ["ResBlock", "filters", "kernel_size"], 
		"maxpool": ["MaxPooling2D", "pool_size", "strides", "padding"], 
		"avgpool": ["AveragePooling2D", "pool_size", "strides", "padding"], 
		"dropout": ["Dropout", "rate"], 
		"dense": ["Dense", "units", "activation"]
	}
}