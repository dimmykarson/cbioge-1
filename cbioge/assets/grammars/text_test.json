{
    "name": "gcn_grammar",

    "blocks": {
        "input": ["Input", "shape"],
        "gconv": ["GraphConv", "units", "activation"],
        "gsconv": ["GraphSageConv", "units", "activation"],
        "gatt": ["GraphAttention", "units", "activation"],
        "aconv": ["ARMAConv", "units", "activation"],
        "appnp": ["APPNP", "units", "activation"],
        "dropout": ["Dropout", "rate"]
    },
    "rules": {
        "<gcn>" : [["<layer_c>", "<dense>"]],
        "<layer_c>": [
            ["<conv>"],
            ["<dropout>"],
            ["<layer_c>", "<layer_c>"]
    ],
    "<dense>": [["<conv_type>", "<units>", "<dense_activation>"]],
    "<conv>": [["<conv_type>", "<units>", "<activation>"]],
    "<conv_type>": [["gconv"], ["gsconv"], ["gatt"], ["aconv"], ["appnp"] ],
    "<dropout>": [["dropout", "<rate>"]],
    "<units>": [[32], [64], [128], [256]],
    "<activation>": [["relu"], ["sigmoid"]],
    "<dense_activation>": [["softmax"]],
    "<rate>": [[0.25], [0.33], [0.5]]
    }
}